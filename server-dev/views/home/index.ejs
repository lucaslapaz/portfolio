    <script>
        let viewMore = null;
        let aboutMeHidden = null;
        

        function viewMoreFunc() {
            if (!viewMore || !aboutMeHidden) return;

            let isHidden = aboutMeHidden.getAttribute('data-hidden');

            if (isHidden === "true") {
                aboutMeHidden.setAttribute('data-hidden', 'false');
                viewMore.textContent = "View less";
            } else {
                aboutMeHidden.setAttribute('data-hidden', 'true');
                viewMore.textContent = "View more";
            }
        }
        
        document.addEventListener("DOMContentLoaded", () => {
            viewMore = document.querySelector('.about-me__view-more');
            aboutMeHidden = document.querySelector('.about-me__hidden');
            viewMore.addEventListener('click', viewMoreFunc)
        })

